<launch>
    <include file="$(find bitbots_speaker)/launch/speaker.launch"/>

    <node name="buttons" pkg="bitbots_buttons" type="button_node.py" />

    <node name="pause" pkg="bitbots_pause" type="pause.py"/>

    <include file="$(find bitbots_animation)/launch/animation.launch"/>

    <include file="$(find bitbots_walking)/launch/walking.launch"/>

    <rosparam file="$(find bitbots_motion)/config/motion.yaml" command="load"/>
    <rosparam file="$(find bitbots_motion)/config/motion_state_machine.yaml" command="load"/>
    <node name="motion" pkg="bitbots_motion" type="motion.py" args="" output="screen" respawn="false"/>

</launch>